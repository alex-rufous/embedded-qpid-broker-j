{
  "name": "${broker.name}",
  "modelVersion": "7.1",
  "authenticationproviders" : [
    {
      "secureOnlyMechanisms": [],
      "name": "plain",
      "type": "Plain",
      "users": [
        {
          "id": "03eb68b8-8e9c-42e0-a7d0-70d436ba22af",
          "type": "managed",
          "name": "${qpid.user.name}",
          "password": "${qpid.user.password}"
        }
      ]
    }],
  "ports" : [  {
    "name" : "AMQP",
    "port" : "${qpid.amqp_port}",
    "authenticationProvider" : "plain",
    "virtualhostaliases" : [ {
      "name" : "nameAlias",
      "type" : "nameAlias"
    }, {
      "name" : "defaultAlias",
      "type" : "defaultAlias"
    }, {
      "name" : "hostnameAlias",
      "type" : "hostnameAlias"
    } ]
  }],
  "virtualhostnodes" : [ {
    "name" : "default",
    "type" : "JSON",
    "defaultVirtualHostNode" : "true",
    "virtualHostInitialConfiguration" : "{\"type\": \"BDB\"}",
    "preferenceStoreAttributes": {"type": "Noop"}
  }]
}
